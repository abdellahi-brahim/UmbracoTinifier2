  <metadata>
    <!-- Required elements-->
    <id>Tinifier</id>
    <version>2.0.2</version>
    <description>The official and free Umbraco TinyPNG package for image compressing and minifying.</description>
    <authors>Backend Devs</authors>
    <dependencies>      
	  <group targetFramework="net471" >
		<dependency id="UmbracoFileSystemProviders.Azure" version="2.0.0" />
	  </group>
    </dependencies>
    <!-- Optional elements -->
    <!-- ... -->
